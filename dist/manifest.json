    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html",
        "default_title": "BroCal",
        "default_icon": "icon.png"

    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }

}