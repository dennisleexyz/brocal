{
    "manifest_version": 3,
    "name": "BroCal",
    "description": "Creates calendar based on your CPP schedule",
    "version": "0.1.0",
    "content_scripts": [
        {
          "matches": ["*://my.cpp.edu/uPortal/f/home_student/normal/render.uP"],
          "js": ["content_script/content_script.js"]
        }
      ],
      
    "permissions": [
      "webRequest"
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }

}